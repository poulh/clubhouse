{
  "name": "event",
  "plural": "events",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "date",
      "required": true
    },
    "locked": {
      "type": "boolean",
      "required": true,
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "members": {
      "type": "hasMany",
      "model": "member",
      "foreignKey": "eventId",
      "options": {
        "nestRemoting": true
      },
      "through": "checkin"
    }
  },
  "acls": [],
  "methods": {}
}
