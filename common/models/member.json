{
  "name": "Member",
  "plural": "members",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "email": {
      "type": "string",
      "required": true,
      "index": {
        "unique": true
      }
    },
    "membershipLevel": {
      "type": "string"
    },
    "homePhone": {
      "type": "string"
    },
    "mobilePhone": {
      "type": "string"
    },
    "workPhone": {
      "type": "string"
    },
    "otherPhone": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "events": {
      "type": "hasMany",
      "model": "Event",
      "foreignKey": "memberId",
      "options": {
        "nestRemoting": true
      },
      "through": "Checkin"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "property": "find",
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "property": "findOne",
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "property": "replaceOrCreate",
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}